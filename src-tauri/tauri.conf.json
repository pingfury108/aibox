{
    "build": {
        "beforeBuildCommand": "cd ./ui && yarn run build",
        "beforeDevCommand": "cd ./ui && yarn run dev",
        "devPath": "http://localhost:5173",
        "distDir": "../ui/dist",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "aibox",
        "version": "0.0.1"
    },
    "tauri": {
        "allowlist": {
            "all": false
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "github.pingfury108.aibox",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
            "fullscreen": false,
            "height": 800,
            "resizable": true,
            "title": "aibox",
            "width": 1000
        }
        ]
    }
}
